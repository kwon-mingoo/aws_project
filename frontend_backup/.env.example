# React Frontend Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_BASE_URL=/api

# AWS Configuration (for frontend AWS SDK usage)
REACT_APP_AWS_REGION=ap-northeast-2

# Application Configuration
REACT_APP_APP_NAME=AWS2-GIOT Application
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_CHATBOT=true
REACT_APP_ENABLE_QUICKSIGHT=true
REACT_APP_ENABLE_S3_UPLOAD=true

# External Services
# REACT_APP_GOOGLE_ANALYTICS_ID=
# REACT_APP_SENTRY_DSN=

# Development
GENERATE_SOURCEMAP=false
DISABLE_ESLINT_PLUGIN=true