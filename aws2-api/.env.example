# NestJS Backend Environment Variables

# Application
NODE_ENV=production
PORT=3001

# AWS Configuration
AWS_REGION=ap-northeast-2
AWS_ACCOUNT_ID=123456789012

# S3 Configuration
S3_BUCKET_NAME=your-s3-bucket-name

# API Configuration
API_URL=http://localhost:3001
FRONTEND_URL=http://localhost

# Database (if needed)
# DATABASE_URL=
# DATABASE_HOST=
# DATABASE_PORT=5432
# DATABASE_USER=
# DATABASE_PASSWORD=
# DATABASE_NAME=

# Redis (if needed)
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=

# JWT Configuration (if needed)
# JWT_SECRET=your-jwt-secret-key
# JWT_EXPIRES_IN=24h

# Logging
LOG_LEVEL=info

# External APIs (if needed)
# EXTERNAL_API_KEY=
# EXTERNAL_API_URL=

# Email (if needed)
# SMTP_HOST=
# SMTP_PORT=587
# SMTP_USER=
# SMTP_PASS=

# Other services
# QUICKSIGHT_DASHBOARD_ID=
# CHATBOT_MODEL_CONFIG=